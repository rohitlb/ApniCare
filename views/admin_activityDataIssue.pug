doctype html
html
    head
    body
        h3 Admin/Activity Queue/ Data issue

        br
        br
        table
            h3 Name &nbsp&nbsp&nbsp&nbsp&nbsp Number &nbsp&nbsp&nbsp&nbsp&nbsp
                span Categories &nbsp&nbsp&nbsp&nbsp&nbsp Data issue &nbsp&nbsp&nbsp&nbsp&nbsp Generated by

            each details in data.Doctor
                p

                    each type in details.issues
                        span  Dr #{details.name} &nbsp&nbsp
                        span #{details.number} &nbsp&nbsp&nbsp&nbsp
                        span  #{type.issueType} &nbsp&nbsp&nbsp&nbsp
                        span
                            a(href="/adminDataIssueInfo?info="+type.issueInfo) Show Issue
                        span    &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp
                        span  #{type.issueFrom} &nbsp&nbsp&nbsp&nbsp
                        br
                        br

            each details in data.Pharma
                p
                    each type in details.issues
                        span  DRx #{details.name} &nbsp&nbsp
                        span #{details.number} &nbsp&nbsp&nbsp&nbsp
                        span #{type.issueType} &nbsp&nbsp&nbsp&nbsp
                        span
                            a(href="/adminDataIssueInfo?info="+type.issueInfo) Show Issue
                        span    &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp
                        span  #{type.issueFrom} &nbsp&nbsp&nbsp&nbsp
                        br
                        br


