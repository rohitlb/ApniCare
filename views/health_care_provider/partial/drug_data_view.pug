            div.col.grey.lighten-3.s12.m12.l10.offset-l1
                each val in data
                    h3 #{val.brand_name.toUpperCase()}
                        span <em>&nbsp - &nbsp</em>
                        each company in val.company_id
                            span <strong>manufactured by #{company.company_name.toUpperCase()}</strong>
                        h5 [#{val.categories}]
                        if(val.type)
                            h5.right #{val.type}
                        br
                        li Consumed in the form of:
                            each dosage in val.dosage_id
                                    br
                                    span
                                        | <strong>#{val.brand_name} #{dosage.dosage_form.toUpperCase()}</strong> &nbsp &nbsp
                                        each dose in dosage.strength_id
                                            span <strong>#{dose.strength}</strong> &nbsp &nbsp
                                            span <strong>#{dose.packaging}</strong> &nbsp &nbsp
                                            span <strong>#{dose.price}</strong>
                                            //each dose in dosage.strength_id


                each vas in data
                    each dosages in vas.dosage_id
                        each doses in dosages.strength_id
                            each pots in doses.potent_substance
                                br
                                li Involves Potent Substances: &nbsp
                                    span <strong>#{pots.name}</strong> &nbsp
                                    span <strong>#{pots.molecule_strength}</strong>
                each vas in data
                    each dosages in vas.dosage_id
                        each doses in dosages.strength_id
                            each pots in doses.potent_substance
                                br
                                if(doses.prescription != '')
                                    li For Consumption of <strong>#{vas.brand_name.toUpperCase()}</strong>, a Prescription is <strong>#{doses.prescription}</strong>
                each vas in data
                    each dosages in vas.dosage_id
                        each doses in dosages.strength_id
                            each pots in doses.potent_substance
                                br
                                if(doses.dose_taken != '')
                                    li Dose Taken As:
                                        br
                                        span <strong>#{doses.dose_taken}</strong>
                each vas in data
                    each dosages in vas.dosage_id
                        each doses in dosages.strength_id
                            each pots in doses.potent_substance
                                br
                                if(doses.dose_timing != '')
                                    li Dose Timings:
                                        br
                                        span <strong>#{doses.dose_timing}</strong>
                each vas in data
                    each dosages in vas.dosage_id
                        each doses in dosages.strength_id
                            each pots in doses.potent_substance
                                br
                                if(vas.primarily_used_for != '')
                                    li The Drug <strong>#{vas.brand_name.toUpperCase()}</strong> is primarily used for <strong>#{vas.primarily_used_for}</strong>
                each vas in data
                    each dosages in vas.dosage_id
                        each doses in dosages.strength_id
                            each pots in doses.potent_substance
                                br
                                if(doses.warnings != '')
                                    i.material-icons.left warning
                                    span.red-text Warnings
                                    br
                                    span <strong>#{doses.warnings}</strong>
                                    br

            div.col.s12.l12.m12.grey.lighten-1(style='padding:10px')
                a.black-text(href='#') <strong>Give a Feedback on the above data</strong>
                    i.material-icons.left.black-text feedback
